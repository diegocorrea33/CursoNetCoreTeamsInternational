@page
@model TGL.WebApp.Pages.Students.EditModel
@{
    ViewData["Title"] = "Edit";
}
<h1>Edit</h1>

<form method="post">
    <input hidden asp-for="Student.Id" />
    Name
    <input asp-for="Student.Name" />
    Last Name
    <input asp-for="Student.LastName" />
    Nit
    <input asp-for="Student.Nit" />
    Age
    <input asp-for="Student.Age" />

    <button class="waves-effect waves-light btn-small" type="submit">
        Edit
    </button>
</form>



<a asp-page="./Index">Return</a>

